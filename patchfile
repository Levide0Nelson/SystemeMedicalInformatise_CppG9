+void Systeme::programmerConsultation() {
+    if (!utilisateurActuel) {
+        cout << "Aucun utilisateur connecté\n";
+        return;
+    }
+    if (dynamic_cast<ProfessionnelSante*>(utilisateurActuel) == nullptr) {
+        cout << "Erreur : Seul un professionnel de santé peut programmer une consultation.\n";
+        return;
+    }
+
+    int idPatient;
+    cout << "\nID du Patient concerne: ";
+    if (!(cin >> idPatient)) { cin.clear(); cin.ignore(numeric_limits<streamsize>::max(), '\n'); return; }
+
+    DossierMedical* dm = trouverDossier(idPatient);
+    if (!dm) {
+        cout << "Dossier non trouvé.\n";
+        return;
+    }
+
+    // Exemple de création d'une consultation minimale (simulation)
+    Date d; // nécessite adaptation selon Date API
+    ProfessionnelSante* ps = dynamic_cast<ProfessionnelSante*>(utilisateurActuel);
+    Consultation c(genererIdConsultation(), d, ps ? ps->getIdUser() : 0, idPatient, Specialite::MEDECINE_GENERALE, "Motif", "Observations");
+    // si DossierMedical::ajouterConsultation est disponible, décommentez :
+    // dm->ajouterConsultation(c);
+
+    cout << "Consultation enregistrée (simulation).\n";
+}
+
+void Systeme::afficherStatistiques() {
+    int nbPatients = 0;
+    int nbPS = 0;
+    int nbConsultations = 0;
+    int nbDossiersArchives = 0;
+    map<Specialite, int> consultationsParSpecialite;
+    map<int, int> consultationsParPS;
+
+    cout << COULEUR_JAUNE << "\n--- STATISTIQUES DU SYSTEME ---" << COULEUR_RESET << endl;
+    cout << "Patients enregistres : " << nbPatients << endl;
+    cout << "Professionnels de Sante : " << nbPS << endl;
+    cout << "Consultations totales : " << nbConsultations << endl;
+    cout << "Dossiers Medicaux Archives : " << nbDossiersArchives << endl;
+    cout << "========================================" << endl;
+    cout << "[INFO] En attente des donnees du Backend (Membres 3, 4, 5)...\n";
+}
